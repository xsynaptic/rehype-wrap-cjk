{
  "name": "rehype-wrap-cjk",
  "version": "1.0.3",
  "description": "Rehype plugin for wrapping CJK characters in an HTML element with a language attribute.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "build-tsc": "tsc --build",
    "check-types": "tsc --project tsconfig.json --noemit",
    "lint": "eslint",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xsynaptic/rehype-wrap-cjk.git"
  },
  "keywords": [
    "unified",
    "rehype",
    "rehype-plugin",
    "cjk"
  ],
  "author": "Alexander Synaptic <x@spectralcodex.com>",
  "bugs": {
    "url": "https://github.com/xsynaptic/rehype-wrap-cjk/issues"
  },
  "homepage": "https://github.com/xsynaptic/rehype-wrap-cjk#readme",
  "license": "MIT",
  "dependencies": {
    "hast-util-is-element": "^3.0.0",
    "hastscript": "^9.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/hast": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/unist": "^3.0.3",
    "eslint": "^9.38.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tsdown": "^0.15.10",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vitest": "^4.0.3"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
