{
  "name": "rehype-wrap-cjk",
  "version": "0.2.1",
  "description": "Rehype plugin for wrapping CJK characters in an element with a language attribute.",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build-tsc": "tsc --build",
    "check-types": "tsc --project tsconfig.json --noemit",
    "lint": "eslint",
    "test": "vitest",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xsynaptic/rehype-wrap-cjk.git"
  },
  "keywords": [
    "unified",
    "rehype",
    "rehype-plugin",
    "cjk"
  ],
  "author": "Alexander Synaptic <x@spectralcodex.com>",
  "bugs": {
    "url": "https://github.com/xsynaptic/rehype-wrap-cjk/issues"
  },
  "homepage": "https://github.com/xsynaptic/rehype-wrap-cjk#readme",
  "license": "MIT",
  "dependencies": {
    "hast-util-is-element": "^3.0.0",
    "hastscript": "^9.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/hast": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/unist": "^3.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.12.2+sha256.2ef6e547b0b07d841d605240dce4d635677831148cd30f6d564b8f4f928f73d2"
}
