{
  "name": "rehype-wrap-cjk",
  "version": "1.0.1",
  "description": "Rehype plugin for wrapping CJK characters in an HTML element with a language attribute.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "build-tsc": "tsc --build",
    "check-types": "tsc --project tsconfig.json --noemit",
    "lint": "eslint",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xsynaptic/rehype-wrap-cjk.git"
  },
  "keywords": [
    "unified",
    "rehype",
    "rehype-plugin",
    "cjk"
  ],
  "author": "Alexander Synaptic <x@spectralcodex.com>",
  "bugs": {
    "url": "https://github.com/xsynaptic/rehype-wrap-cjk/issues"
  },
  "homepage": "https://github.com/xsynaptic/rehype-wrap-cjk#readme",
  "license": "MIT",
  "dependencies": {
    "hast-util-is-element": "^3.0.0",
    "hastscript": "^9.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/hast": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/unist": "^3.0.3",
    "eslint": "^9.35.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
